<h1>All Polishes</h1>
<% @polishes.each do |polish| %>
<div class="media">
	<div class="media-body">
		<h4 class="media-heading">
			<%= link_to polish.name, polish %>
		</h4>
		<small>
			submitted <%= time_ago_in_words(polish.created_at) %> ago by <%= polish.user.name %>
		</small>
	</div>
</div>
<% end %>